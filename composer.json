{
  "name": "meanbee/magedbm2",
  "description": "Magento 2 database backup and import tool",
  "homepage": "https://github.com/meanbee/magedbm2",
  "license": "OSL-3.0",
  "keywords": [
    "magento2",
    "magento",
    "database",
    "backup",
    "magedbm2",
    "magedbm"
  ],
  "authors": [
    {
      "name": "Tomas Gerulaitis",
      "email": "tomas.gerulaitis@meanbee.com"
    }
  ],
  "bin": [
    "bin/magedbm2"
  ],
  "require": {
    "php": ">=7.0.0",
    "symfony/console": "^2.8.20",
    "symfony/yaml": "^2.8.20",
    "aws/aws-sdk-php": "^3.28.0",
    "n98/magerun2": "^1.4.0"
  },
  "require-dev": {
    "phing/phing": "^2.15.2",
    "phpunit/phpunit": "^6.1.4",
    "squizlabs/php_codesniffer": "^3.0.0",
    "phpmd/phpmd": "^2.6.0"
  },
  "autoload": {
    "psr-4": {
      "Meanbee\\Magedbm2\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Meanbee\\Magedbm2\\Tests\\": "tests/"
    }
  }
}
